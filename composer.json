{
  "name": "gerpo/dms",
  "description": "DMS - Dormitory Management System",
  "keywords": ["DMS", "dorm", "dormitory", "management system", "laravel"],
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "type": "vcs",
      "url": "D:\\Web-Projekte\\plugisto\\packages\\gerpo\\plugisto"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "fideloper/proxy": "^4.0",
    "gerpo/plugisto": "*",
    "laravel/framework": "5.6.*",
    "laravel/tinker": "^1.0",
    "silber/bouncer": "v1.0.0-rc.1",
    "tightenco/ziggy": "^0.6.3"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.1",
    "barryvdh/laravel-ide-helper": "^2.4",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "laravel/dusk": "^3.0",
    "martinlindhe/laravel-vue-i18n-generator": "^0.1.28",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0",
    "symfony/thanks": "^1.0",
    "wnx/laravel-stats": "^1.6"
  },
  "autoload": {
    "classmap": ["database/seeds", "database/factories"],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": ["tests/utilities/functions.php"]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": ["@php artisan key:generate"],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover",
      "php artisan plugisto:build",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
